{
  "name": "@outloud/eslint-config",
  "version": "2.0.4",
  "description": "ESLint config for Outloud projects.",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madebyoutloud/eslint-config.git"
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepack": "is-ci || pnpm build",
    "release": "release-it",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.2.4",
    "@nuxt/eslint-plugin": "^1.3.0",
    "@outloud/eslint-config": "workspace:*",
    "@release-it/conventional-changelog": "^7.0.2",
    "@types/node": "^20.12.7",
    "@unocss/eslint-config": "66.1.0-beta.7",
    "eslint": "^9.23.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vue": "^10.0.0",
    "is-ci": "^4.1.0",
    "release-it": "^16.2.1",
    "tsup": "^8.0.2",
    "typescript": "^5.7.3",
    "vue-eslint-parser": "^10.1.1"
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.0.0",
    "@clack/prompts": "^0.10.0",
    "@eslint/js": "^9.23.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "eslint-config-flat-gitignore": "^2.0.0",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-merge-processors": "^2.0.0",
    "eslint-plugin-check-file": "^3.1.0",
    "eslint-plugin-import-x": "^4.9.4",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-processor-vue-blocks": "^2.0.0",
    "globals": "^16.0.0",
    "local-pkg": "^1.1.1",
    "typescript-eslint": "^8.28.0"
  },
  "peerDependencies": {
    "eslint": "^9"
  }
}
